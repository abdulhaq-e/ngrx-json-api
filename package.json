{
  "name": "ngrx-json-api",
  "version": "1.1.0",
  "description": "A JSON API client for ngrx",
  "main": "index.js",
  "module": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdulhaq-e/ngrx-json-api.git"
  },
  "scripts": {
    "build:js": "ngc -p tsconfig.dist.json",
    "build:umd": "webpack --config webpack.config.js",
    "build:uglify": "webpack --config webpack.prod.config.js",
    "build:inspect": "webpack --json | webpack-bundle-size-analyzer",
    "clean:pre": "rimraf release",
    "clean:post": "rimraf \"src/**/*.ngfactory.ts\"",
    "copy": "cpy package.json README.md release",
    "coveralls": "cat coverage/coverage.lcov | ./node_modules/coveralls/bin/coveralls.js",
    "build": "npm run build:js && npm run build:umd && npm run build:uglify",
    "karma": "karma start",
    "karma:watch": "karma start --no-single-run",
    "lint": "npm run lint:lib && npm run lint:spec",
    "lint:lib": "tslint src/**.ts",
    "lint:spec": "tslint spec/**.ts",
    "prebuild": "npm run test:unit && npm run clean:pre && npm run lint:lib",
    "postbuild": "npm run clean:post && npm run copy",
    "test:unit": "npm run karma",
    "test:unit:watch": "npm run karma:watch",
    "test": "npm run test:unit && npm run lint:lib && npm run coveralls",
    "publish": "npm publish"
  },
  "authors": [
    "Abdulhaq Emhemmed",
    "remmeier"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": "^5.0.0-beta.1",
    "@angular/core": "^5.0.0-beta.1",
    "@angular/http": "^5.0.0-beta.1",
    "@ngrx/effects": "4.0.0",
    "@ngrx/store": "4.0.0",
    "lodash": "^4.17.4",
    "rxjs": "5.4.2"
  },
  "devDependencies": {
    "@angular/common": "^5.0.0-beta.1",
    "@angular/compiler": "^5.0.0-beta.1",
    "@angular/compiler-cli": "^5.0.0-beta.1",
    "@angular/core": "^5.0.0-beta.1",
    "@angular/http": "^5.0.0-beta.1",
    "@angular/platform-browser": "^5.0.0-beta.1",
    "@angular/platform-browser-dynamic": "^5.0.0-beta.1",
    "@ngrx/effects": "4.0.0",
    "@ngrx/store": "4.0.0",
    "@ngtools/webpack": "^1.5.5",
    "@types/deep-freeze": "^0.0.29",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.53",
    "@types/lodash": "^4.14.71",
    "@types/node": "^8.0.17",
    "@types/selenium-webdriver": "^3.0.4",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.29",
    "@types/webpack": "^3.0.5",
    "awesome-typescript-loader": "^3.0.8",
    "babel-core": "^6.14.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.8",
    "babel-preset-es2015": "^6.14.0",
    "babili": "0.1.2",
    "babili-webpack-plugin": "0.0.8",
    "codelyzer": "^3.0.1",
    "conventional-changelog-cli": "^1.1.1",
    "core-js": "^2.4.1",
    "coveralls": "^2.11.15",
    "cpy-cli": "^1.0.1",
    "deep-freeze": "^0.0.1",
    "expand-range": "^2.0.1",
    "filename-regex": "^2.0.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.4.1",
    "jasmine-marbles": "^0.1.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-diff-reporter": "^0.6.3",
    "karma-mocha-reporter": "^2.1.0",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "loader-utils": "^1.0.2",
    "lodash": "^4.17.2",
    "lodash-webpack-plugin": "^0.11.4",
    "lru-cache": "^4.1.1",
    "npm-run-all": "^4.0.1",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.5.4",
    "rxjs": "^5.4.2",
    "source-map-loader": "^0.2.1",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "ts-helpers": "1.1.2",
    "tslint": "~5.0.0",
    "typescript": "2.3.4",
    "webpack": "3.4.1",
    "webpack-bundle-size-analyzer": "2.7.0",
    "zone.js": "0.8.12"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
